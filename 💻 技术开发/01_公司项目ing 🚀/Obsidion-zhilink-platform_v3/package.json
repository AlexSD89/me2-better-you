{
  "name": "zhilink-v3",
  "version": "0.1.0",
  "description": "LaunchX智链平台v3 - B2B AI能力市场平台",
  "private": true,
  "scripts": {
    "dev": "next dev --port 1300",
    "dev:turbo": "next dev --turbo --port 1300",
    "build": "next build",
    "start": "next start --port 1300",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "check": "npm run lint && npm run typecheck",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "cross-env ANALYZE=true next build",
    "component:showcase": "next dev --port 1301",
    "type:generate": "typescript-json-schema src/types/index.ts '*' --out src/types/schema.json",
    "docs:generate": "typedoc src/components --out docs/components",
    "docs:organize": "node scripts/intelligent-docs-organizer.js",
    "docs:health-check": "node scripts/docs-health-check.js",
    "docs:full-check": "npm run docs:organize && npm run docs:health-check",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "clean": "rm -rf .next dist coverage storybook-static",
    "reset": "npm run clean && rm -rf node_modules && npm install"
  },
